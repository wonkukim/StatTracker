  <p>
    <%= f.label :games_started %><br />
    <%= f.text_field :games_started %>
  </p>
  <p>
    <%= f.label :inning_outs %><br />
    <%= f.text_field :inning_outs %>
  </p>
  <p>
    <%= f.label :chances %><br />
    <%= f.text_field :chances %>
