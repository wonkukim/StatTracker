<%= form_for @fielding_stat do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :player_id %><br />
    <%= f.text_field :player_id %>
  </p>
  <p>
    <%= f.label :team_id %><br />
    <%= f.text_field :team_id %>
  </p>
  <p>
    <%= f.label :stint %><br />
    <%= f.text_field :stint %>
  </p>
  <p>
    <%= f.label :position %><br />
    <%= f.text_field :position %>
