<% title "Fielding Post Stats" %>

<table>
  <tr>
    <th>Player</th>
    <th>Team</th>
    <th>Round</th>
    <th>Position</th>
    <th>Games</th>
    <th>Games Started</th>
    <th>Inning Outs</th>
    <th>Chances</th>
    <th>Put Outs</th>
    <th>Assists</th>
    <th>Errors Made</th>
    <th>Double Plays</th>
    <th>Triple Plays</th>
    <th>Passed Balls</th>
    <th>Stolen Bases</th>
    <th>Caught Stealing</th>
  </tr>
  <% for fielding_post_stat in @fielding_post_stats %>
    <tr>
      <td><%= fielding_post_stat.player_id %></td>
      <td><%= fielding_post_stat.team_id %></td>
      <td><%= fielding_post_stat.round %></td>
      <td><%= fielding_post_stat.position %></td>
      <td><%= fielding_post_stat.games %></td>
      <td><%= fielding_post_stat.games_started %></td>
      <td><%= fielding_post_stat.inning_outs %></td>
      <td><%= fielding_post_stat.chances %></td>
      <td><%= fielding_post_stat.put_outs %></td>
      <td><%= fielding_post_stat.assists %></td>
      <td><%= fielding_post_stat.errors_made %></td>
      <td><%= fielding_post_stat.double_plays %></td>
      <td><%= fielding_post_stat.triple_plays %></td>
      <td><%= fielding_post_stat.passed_balls %></td>
      <td><%= fielding_post_stat.stolen_bases %></td>
      <td><%= fielding_post_stat.caught_stealing %></td>
      <td><%= link_to "Show", fielding_post_stat %></td>
      <td><%= link_to "Edit", edit_fielding_post_stat_path(fielding_post_stat) %></td>
      <td><%= link_to "Destroy", fielding_post_stat, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New Fielding Post Stat", new_fielding_post_stat_path(fielding_post_stat) %></p>
